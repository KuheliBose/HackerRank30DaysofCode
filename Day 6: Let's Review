#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int N;
    std::cin >> N;
    
    for(int i = 0; i<N; i++){
        string str;
        std::cin >> str;
    for(int index = 0; index < str.length(); index += 2)
        {
        std::cout << str[index];
        }
        std::cout << " ";
    for(int index = 1; index < str.length(); index += 2)
        {
        std::cout << str[index];}
        std::cout << std::endl;
    }
        return 0;
}
